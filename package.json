{
  "name": "NodeTS",
  "version": "0.0.0",
  "scripts": {
    "dev": "npm run build-ts && npm run dev:watch",
    "dev:watch": "concurrently --kill-others -p \"[{name}]\" -n \"TypeScript,Node\" -c \"yellow.bold,cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run watch-node\"",
    "watch-ts": "tsc -w",
    "build-ts": "tsc",
    "watch-node": "nodemon --delay 1 --watch dev --exec npm run start:dev",
    "build": "npm run build-ts",
    "start:dev": "cross-env NODE_ENV=development DISABLE_LOGGING=false node dev/index.js",
    "test": "jest --forceExit --detectOpenHandles",
    "lint": "eslint typings/**/* src/**/* test/**/* --fix"
  },
  "dependencies": {
    "bcrypt": "^5.0.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.1",
    "command-line-args": "^5.2.1",
    "compression": "^1.7.4",
    "concurrently": "^7.1.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.0",
    "envalid": "^7.1.0",
    "eslint": "^7.32.0",
    "eslint-plugin-more": "^1.0.5",
    "express": "^4.17.3",
    "express-async-errors": "^3.1.1",
    "helmet": "^5.0.2",
    "hpp": "^0.2.3",
    "http-status-codes": "^2.2.0",
    "jet-logger": "^1.1.5",
    "jsonfile": "^6.1.0",
    "jsonwebtoken": "^8.5.1",
    "module-alias": "^2.2.2",
    "morgan": "^1.10.0",
    "pg": "^8.7.3",
    "reflect-metadata": "^0.1.13",
    "routing-controllers": "^0.9.0",
    "swagger-jsdoc": "^6.0.0",
    "swagger-ui-express": "^4.1.6",
    "typeorm": "^0.2.34",
    "typeorm-typedi-extensions": "^0.4.1"
  },
  "devDependencies": {
    "@swc/cli": "^0.1.51",
    "@swc/core": "^1.2.108",
    "@types/bcrypt": "^5.0.0",
    "@types/command-line-args": "^5.2.0",
    "@types/compression": "^1.7.1",
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/find": "^0.2.1",
    "@types/fs-extra": "^9.0.13",
    "@types/hpp": "^0.2.1",
    "@types/jasmine": "^4.0.3",
    "@types/jest": "27.4.x",
    "@types/jsonfile": "^6.1.0",
    "@types/jsonwebtoken": "^8.5.4",
    "@types/morgan": "^1.9.3",
    "@types/node": "^17.0.25",
    "@types/supertest": "^2.0.12",
    "@types/swagger-jsdoc": "^6.0.1",
    "@types/swagger-ui-express": "^4.1.3",
    "@types/validator": "^13.7.2",
    "@typescript-eslint/eslint-plugin": "^5.20.0",
    "@typescript-eslint/parser": "^5.20.0",
    "cross-env": "^7.0.3",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-config-node": "^4.1.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.4.1",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-react-hooks": "^4.5.0",
    "eslint-plugin-unicorn": "^33.0.1",
    "find": "^0.3.0",
    "fs-extra": "^10.1.0",
    "husky": "^7.0.1",
    "jasmine": "^4.1.0",
    "jest": "^27.5.1",
    "lint-staged": "^11.0.0",
    "node-config": "^0.0.2",
    "node-gyp": "^8.1.0",
    "nodemon": "^2.0.15",
    "pm2": "^5.1.0",
    "prettier": "^2.6.2",
    "supertest": "^6.2.2",
    "ts-jest": "^27.1.4",
    "ts-node": "^10.7.0",
    "tsc-alias": "^1.4.1",
    "tsconfig-paths": "^3.14.1",
    "typescript": "^4.6.3"
  },
  "lint-staged": {
    "*.{ts,js}": [
      "npm run lint -- --cache --fix"
    ]
  }
}
